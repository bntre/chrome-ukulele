{
  "manifest_version": 2,
  "name": "Ukulele Chords",
  "version": "0.1",
  "description": "Ukulele Chords for AmDm.ru",
  "content_scripts": [{
    "matches": ["https://amdm.ru/akkordi/*/*"],
    "js": ["content.js"]
  }],
  "permissions": ["storage"],
  "web_accessible_resources": ["chords/*.png"],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}